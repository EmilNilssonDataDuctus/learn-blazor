        <MudTreeViewItem Value="@("Livets mat AB")" />
        <MudTreeViewItem Value="@("Restaurang Skjulet")">
            <MudTreeViewItem Value="@("Skjulet Mat")" />
            <MudTreeViewItem Value="@("Skjulet Baren")" />
            <MudTreeViewItem Value="@("Skjulet Frukost")" />
